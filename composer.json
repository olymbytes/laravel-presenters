{
    "name": "olymbytes/laravel-presenters",
    "description": "Add presenters and presentable behaviour to your Laravel app",
    "homepage": "https://github.com/olymbytes/laravel-presenters",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@foreno.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/console": "^5.5"
    },
    "require-dev": {
        "orchestra/database": "^3.5",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Olymbytes\\Presenters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olymbytes\\Presenters\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olymbytes\\Presenters\\PresentersServiceProvider"
            ]
        }
    }
}
